<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/</from>
		<to type="redirect" last="true">%{context-path}/app</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>